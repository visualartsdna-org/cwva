voc:catsTongue
	a skos:Concept ;
	skos:prefLabel "Cat's Tongue" ;
	skos:broader voc:watercolorTool ;
	skos:definition "Brush type; similar in shape to a Filbert brush, but with a pointed tip. The ideal tool for creating single-stroke flower petals and leaves.To help clarify the definitions of specialty brushes in this glossary, use this visual guide to basic brush shapes as a reference." ;
	skos:notation "b084ec7a-bef0-4fc0-a9a8-b37e419aadbd" ;
	skos:inScheme voc:watercolorPainting ;																				
	.


voc:cauliflower
	a skos:Concept ;
	skos:prefLabel "Cauliflower" ;
	skos:broader voc:watercolorTerm ;
	skos:definition "(See Backrun)" ;
	skos:notation "698a5086-a2ac-4817-bf23-0eb5372efff5" ;
	skos:inScheme voc:watercolorPainting ;																				
	.


voc:charging
	a skos:Concept ;
	skos:prefLabel "Charging" ;
	skos:broader voc:watercolorTechnique ;
	skos:definition "Is a technique that involves dropping a new wet color into an existing wash allowing them to blend ad create interesting variations of color or tonal value." ;
	skos:definition "technique that involves mixing two or more colors directly on the paper instead of premixing on a palette" ;
	skos:notation "2aa59820-ad46-40f3-a929-bfd694017a7d" ;
	skos:inScheme voc:watercolorPainting ;																				
	.


