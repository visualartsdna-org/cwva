	a skos:Concept ;
	skos:prefLabel "pan paint" ;
	skos:broader voc:watercolorMaterial ;
	skos:definition "small cakes of watercolor paint that come in half-pan or whole-pan sizes" ;
	skos:notation "e0fde960-13c1-4ba1-a212-569e6c0c2307" ;
	skos:inScheme voc:watercolorPainting ;																				
	.


voc:pan
	a skos:Concept ;
	skos:prefLabel "Pan" ;
	skos:broader voc:watercolorTool ;
	skos:definition "Pans are small thumbnail sized paint containers that slot into portable trays. Pans are most often used for painting in small sketchbooks and outdoors" ;
	skos:notation "b63c1c11-8d41-499a-aad3-b3a2ce83b52a" ;
	skos:inScheme voc:watercolorPainting ;																				
	.


voc:pigment
	a skos:Concept ;
	skos:prefLabel "Pigment" ;
	skos:broader voc:watercolorMaterial ;
	skos:definition "Pigments is the substance that gives paint its color. Pigments can be natural or synthetic" ;
	skos:definition "dry coloring matter, usually an insoluble powder, that’s mixed with water and gum Arabic to create paint" ;
	skos:notation "09041a6f-362d-43a6-8afd-d00ad9b12bc0" ;
	skos:inScheme voc:watercolorPainting ;																				
	.


