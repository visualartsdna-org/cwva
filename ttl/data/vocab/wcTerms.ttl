	a skos:Concept ;																									
	skos:prefLabel "watercolor material" ;																									
	skos:definition "A material used in watercolor painting." ;																									
	skos:notation "24eb6db8-cef8-4ff8-9d68-8529aec6cf00" ;
	skos:inScheme voc:watercolorPainting ;																				
	.
voc:watercolorTool																										
	a skos:Concept ;																									
	skos:prefLabel "watercolor tool" ;																									
	skos:definition "A tool used in watercolor painting." ;																									
	skos:notation "24eb6db8-cef8-4ff8-9d68-8529aec6cf00" ;
	skos:inScheme voc:watercolorPainting ;																				
	.



voc:abstract
	a skos:Concept ;
	skos:prefLabel "Abstract" ;
	skos:broader voc:watercolorTerm ;
	skos:definition "The term “Abstract” is used to describe a painting that is non-representational. i.e the subject matter of the painting is not about the depiction of reality." ;
	skos:notation "69a7a58a-8ca2-4b85-86de-0c69523ba45c" ;
	skos:inScheme voc:watercolorPainting ;																				
	.


voc:analogousColors
	a skos:Concept ;
	skos:prefLabel "Analogous Colors" ;
	skos:broader voc:watercolorTerm ;
	skos:definition "Analogous colors are colors that are adjacent on the Color Wheel. e.g. blue, blue/green, green." ;
	skos:notation "8e8d4200-3bb8-4e3b-aa1a-139f6a440d1c" ;
	skos:inScheme voc:watercolorPainting ;																				
	.


