voc:syntheticBrushes
	a skos:Concept ;
	skos:prefLabel "Synthetic (Brushes)" ;
	skos:broader voc:watercolorTool ;
	skos:definition "Synthetic brushes can be made from Nylon, Polyester or Taklon. They tend to be stiffer and springier than natural haired brushes." ;
	skos:notation "a5dd74ea-f3df-4fe3-b882-ad8985423cf7" ;
	skos:inScheme voc:watercolorPainting ;																				
	.


voc:temperatureColor
	a skos:Concept ;
	skos:prefLabel "Temperature (Color)" ;
	skos:broader voc:watercolorTerm ;
	skos:closeMatch voc:temperature ;
	skos:definition "Colors are divided into cool and warm. Colors that lean towards Red/Yellow are considered to be warm colors and colors that lean towards Blue/Green are classified as cool." ;
	skos:notation "a2ce14e6-bd59-44ed-ae5d-708532371bfa" ;
	skos:inScheme voc:watercolorPainting ;																				
	.


voc:temperature
	a skos:Concept ;
	skos:prefLabel "temperature" ;
	skos:broader voc:watercolorTerm ;
	skos:closeMatch voc:temperatureColor ;
	skos:definition "the warmness or coolness of a color, depending on where the color is situated on the color wheel" ;
	skos:notation "cca6f7bc-7d9f-474f-93d0-fd030b12c310" ;
	skos:inScheme voc:watercolorPainting ;																				
	.


voc:thumbnail
	a skos:Concept ;
	skos:prefLabel "Thumbnail" ;
	skos:broader voc:watercolorTerm ;
	skos:definition "A small sketch or painting produced in preparation for the creation of a larger work." ;
	skos:notation "9817a1a3-07d6-4bd8-8d56-6104c5a53a9f" ;
	skos:inScheme voc:watercolorPainting ;																				
	.


voc:tint
	a skos:Concept ;
	skos:prefLabel "Tint" ;
	skos:broader voc:watercolorTechnique ;
	skos:definition "Color tints are generally created by adding white to a base color. In watercolor it is done by adding water." ;
	skos:definition "created by adding water to the original color; the more water that’s added, the weaker the intensity" ;
	skos:notation "c564e476-cd43-43bb-89cd-cad427f7f954" ;
	skos:inScheme voc:watercolorPainting ;																				
	.


