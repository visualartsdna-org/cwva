voc:pleinAir
	a skos:Concept ;
	skos:prefLabel "Plein Air" ;
	skos:broader voc:watercolorTerm ;
	skos:definition "Plein Air, or En Plein Air means the act of painting outdoors." ;
	skos:notation "9a15dc12-69a6-41cf-8237-2ae53a072846" ;
	skos:inScheme voc:watercolorPainting ;																				
	.


voc:primaryColor
	a skos:Concept ;
	skos:prefLabel "Primary (Color)" ;
	skos:broader voc:watercolorTerm ;
	skos:definition "Red, Blue and Yellow are called the Primary Colors because they cannot mixed from other colors but all colors can be mixed from them." ;
	skos:notation "3212391c-fdc5-4c58-bfce-b6ca04055d11" ;
	skos:inScheme voc:watercolorPainting ;																				
	.


