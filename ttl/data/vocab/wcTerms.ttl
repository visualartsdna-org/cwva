	skos:prefLabel "lifting paint" ;
	skos:broader voc:watercolorTechnique ;
	skos:closeMatch voc:liftingOut, voc:lifting ;
	skos:definition "a technique for removing paint from a surface with a brush, paper towel or tissue in order to correct mistakes, develop textures, create highlights or change values" ;
