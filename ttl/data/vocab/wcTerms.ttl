voc:atmosphericDisturbance
	a skos:Concept ;
	skos:prefLabel "Atmospheric Disturbance" ;
	skos:broader voc:watercolorTerm ;
	skos:definition "Visual effect; the tendency for objects that are far in the distance to appear out-of-focus and muted in colour due to the natural occurrence of particulates in the atmosphere that mess with the way light refracts into your eye; a fancy way of saying that stuff far away is blurry." ;
	skos:notation "f61ef755-2a6f-408d-a39a-204d6114beca" ;
	skos:inScheme voc:watercolorPainting ;																				
	.


voc:atomiserSprayer
	a skos:Concept ;
	skos:prefLabel "Atomiser sprayer" ;
	skos:broader voc:watercolorTool ;
	skos:definition "Painting tool; manual (mouth operated) paint spraying tool that creates a fine mist and similar painting effect to an airbrush or aerosol spray can; sometimes referred to as a diffuser." ;
	skos:notation "4840175c-0801-4b1d-b70d-796a9dfe71c8" ;
	skos:inScheme voc:watercolorPainting ;																				
	.

voc:atomiser
	a skos:Concept ;
	skos:prefLabel "Atomiser" ;
	skos:broader voc:watercolorTechnique ;
	skos:definition "The process of spraying a fine mist, similar painting effect to an airbrush or aerosol spray can; sometimes referred to as a diffuser." ;
	skos:notation "ff3bbde9-61c3-4c15-92cc-82b3c3de1354" ;
	skos:inScheme voc:watercolorPainting ;																				
	.


voc:backRun
	a skos:Concept ;
	skos:prefLabel "Back Run" ;
	skos:broader voc:watercolorTechnique ;
	skos:closeMatch voc:backrun ;
	skos:definition "A back run occurs when painting into a wet area of watercolor with a mix of paint that is more dilute than is currently there. This causes the paint to spread out (Bloom) and often leaves a cauliflower shaped pattern when dry. This is often considered to be undesirable but can also be done intentionally to create a textural effect." ;
	skos:notation "5cce7d6c-25c1-4620-9b05-2dd0df709a18" ;
	skos:inScheme voc:watercolorPainting ;																				
	.


voc:backrun
	a skos:Concept ;
	skos:prefLabel "Backrun" ;
	skos:broader voc:watercolorTechnique ;
	skos:closeMatch voc:backRun ;
	skos:definition "Painting technique; the mixing of colours in a semi-controlled way by adding new colour to a wash that hasn't dried; backruns can be an unwelcome accident, but are also deliberately created by some artists to represent abstracted clouds, foliage or flowers, or just to create a specific mood and for visual interest." ;
	skos:notation "08ca18e5-e300-4a0b-b2cb-9d8f35865db0" ;
	skos:inScheme voc:watercolorPainting ;																				
	.


voc:bambooBrush
	a skos:Concept ;
	skos:prefLabel "Bamboo Brush" ;
	skos:broader voc:watercolorTool ;
	skos:definition "An inexpensive natural haired chinese style brush (Often squirrel hair) that has a bamboo handle and is mainly used to create loose painterly strokes.I love using this brush for natural looking foliage and skiesI love using this brush for natural looking foliage and skies" ;
	skos:notation "97bbc4a6-b170-4bd5-a44a-1a9e71bcd0a3" ;
	skos:inScheme voc:watercolorPainting ;																				
	.


voc:bead
	a skos:Concept ;
	skos:prefLabel "Bead" ;
	skos:broader voc:watercolorTechnique ;
	skos:definition "A bead is the line of water that pools at the base of a Wash. Maintaining the bead becomes important when attempting to paint a consistent wash as streaks will occur in the if it dries out too quickly." ;
	skos:notation "fc4fb13c-8ec0-4baf-9643-cf839f6aa0d4" ;
	skos:inScheme voc:watercolorPainting ;																				
	.


voc:binder
	a skos:Concept ;
	skos:prefLabel "Binder" ;
	skos:broader voc:watercolorMaterial ;
	skos:definition "Binder is a substance that holds watercolor pigment in suspension. The substance used is generally Gum Arabic or synthetic Glycol." ;
	skos:notation "72ebc54c-a920-43a9-8167-fdef3ec766eb" ;
	skos:inScheme voc:watercolorPainting ;																				
	.


voc:block
	a skos:Concept ;
	skos:prefLabel "Block" ;
	skos:broader voc:watercolorMaterial ;
	skos:definition "Watercolor paper is often sold in a “Block”, which is a number of sheets of paper that are glued around the edges as opposed to loose. This minimizes Cockling and eliminates the need for stretching paper." ;
	skos:notation "dfbfbed8-5f4b-42e8-b4fe-d6a71882ff9e" ;
	skos:inScheme voc:watercolorPainting ;																				
	.


voc:bloom
	a skos:Concept ;
	skos:prefLabel "Bloom" ;
	skos:broader voc:watercolorTechnique ;
	skos:definition "See Backrun" ;
	skos:notation "243e9a3e-e5db-4de4-8ada-104e21b65d28" ;
	skos:inScheme voc:watercolorPainting ;																				
	.


voc:blotting
	a skos:Concept ;
	skos:prefLabel "Blotting" ;
	skos:broader voc:watercolorTechnique ;
	skos:definition "Paper type; Blotter paper; a highly absorbent type of paper used to absorb an excess of liquid (such as ink or paint) from the substrate.ainting technique; Blotting-off; the act of manually soaking up excess paint before it dries using a small sponge, piece of absorbent tissue or a towel" ;
	skos:notation "3f563b12-2a7e-44f2-86bc-2ecb751c08f9" ;
	skos:inScheme voc:watercolorPainting ;																				
	.


voc:bodyColour
	a skos:Concept ;
	skos:prefLabel "Body Colour" ;
	skos:broader voc:watercolorMaterial ;
	skos:definition "Paint type; a slightly confusing term that simply means opaque water-based paint (such as Gouache or Tempera). Opaque colours can be used to add punches of solid colour to transparent watercolours, and opaque white is used to bring highlights back out of muddy or over-saturated areas." ;
	skos:notation "06816e3d-ccaa-453e-954b-d2bc1e840cf8" ;
	skos:inScheme voc:watercolorPainting ;																				
	.


voc:buckle
	a skos:Concept ;
	skos:prefLabel "Buckle" ;
	skos:broader voc:watercolorTerm ;
	skos:definition "wavy, uneven surface once water has been applied; can and should be avoided by stretching the paper prior to painting. (see; stretching)Although some aspects of painting once thought to be negative (such as backruns) can be used to ones advantage, buckling paper is never desired and never considered a happy accident." ;
	skos:notation "7d7a7228-2199-46af-b6c2-c39747b0e6c9" ;
	skos:inScheme voc:watercolorPainting ;																				
	.


voc:canvas
	a skos:Concept ;
	skos:prefLabel "Canvas" ;
	skos:broader voc:watercolorMaterial ;
	skos:definition "Watercolor canvas is similar to oil painting canvas. Watercolor canvas has been treated with Gesso to enable it to take water based paints." ;
	skos:notation "ea6b1e7e-672b-4063-ba59-89d7cd3c0a09" ;
	skos:inScheme voc:watercolorPainting ;																				
	.


voc:casein
	a skos:Concept ;
	skos:prefLabel "Casein" ;
	skos:broader voc:watercolorMaterial ;
	skos:definition "Casein is a water based painting medium derived from milk. It’s properties resemble Gouache." ;
	skos:definition "a fast-drying, water-soluble paint derived from milk protein, or casein" ;
	skos:notation "e9758c98-3e7b-44d1-8bc5-12da4e8d6a72" ;
	skos:inScheme voc:watercolorPainting ;																				
	.


