		sh:minCount 1 ;
		sh:datatype xsd:string ;
		sh:pattern """^[A-Za-z0-9 _\t\n\r\\.,\\:;\\-'’“”"!#\\?\\$\\%\\&\\(\\)/\\\\]+$""" ;
	] ;
	sh:property [
		sh:path rdfs:label ;
